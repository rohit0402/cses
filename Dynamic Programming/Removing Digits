#include <bits/stdc++.h>
using namespace std;
const int mod = 1e9+7;
typedef long long ll;

void solution() {
   ll n;
    cin>>n;
    vector<ll>dp(n+1,INT_MAX);
    dp[0]=0;
    for(int i=1;i<=n;i++){
        ll num=i;
        while(num){
            ll dig=num%10;
            // if(dig==0)cp
            dp[i]=min(dp[i],1+dp[i-dig]);
            num=num/10;
        }
    }
    cout<<dp[n]<<endl;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t=1;
    // cin>>t;
    while(t--){
        solution();
    }
    
    return 0;
} 
